package hola.mundo;

//import java.util.Iterator;
import java.util.Scanner;

public class HolaMundo2 extends Metodos{
	
	
	

	public static void main(String[] args) {
		
		String [] dias = {"lunes",
						  "martes",
						  "Miercoles",
						  "Jueves",
						  "Viernes"				
		};
		
		Metodos met = new Metodos();
		
		int num = met.posicionDia("viernes", dias);
		System.out.println(num);
		
		
	}
}