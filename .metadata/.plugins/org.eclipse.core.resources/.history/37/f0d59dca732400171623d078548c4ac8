package poo;

public class LibroCalificaciones {
	
	private String nombreDelCurso; //variable de instancia
	
	public void establecerNombreDelcurso(String curso){
		
		this.nombreDelCurso = curso;
		
	}
	
	public void devolverNombreDelCurso(){
		
	}
	
	public void mostrarMensaje(String nombreDelCurso){
		System.out.printf("Bienvenido al Libro de calificaciones para %s!\n", nombreDelCurso);
	}

}
