package com.jairo.poo;

public class Principal {

	public static void main(String[] args) {
		
		//Tipo de dato nombreobjt
		
		Vehiculo miVehiculo = new Vehiculo();
		miVehiculo.marca = "Hiundai";
		miVehiculo.modelo = "i10";
		miVehiculo.anio = 2014;
		
		miVehiculo.arrancar();
		miVehiculo.frenar();
		miVehiculo.reverza();
		miVehiculo.arrancar();
		
		

	}

}
